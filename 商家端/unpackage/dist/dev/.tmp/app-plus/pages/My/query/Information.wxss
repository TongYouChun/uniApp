.Information-container {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  background: #F0F4F7;
  padding-bottom: 72rpx;
}
.Information-container .h3 {
    margin: 16rpx 0 19rpx 0;
    width: 710rpx;
    line-height: 90rpx;
    text-align: center;
    background: #18BA42;
}
.Information-container .Order-information {
    padding: 10rpx 14rpx 24rpx 26rpx;
    width: 670rpx;
    background: #fff;
}
.Information-container .Order-information .h4 {
      line-height: 62rpx;
      color: #000;
      border-bottom: 1rpx solid #E6E6E6;
}
.Information-container .Order-information .information {
      margin: 26rpx 24rpx 10rpx 0;
      height: 120rpx;
}
.Information-container .Order-information .information .Commodity-name {
        height: 100%;
}
.Information-container .Order-information .information .Commodity-name .commodity {
          width: 468rpx;
}
.Information-container .Order-information .information .Commodity-name .commodity .img {
            width: 120rpx;
            height: 120rpx;
            border: 1rpx solid #D4D2D2;
            border-radius: 10rpx;
}
.Information-container .Order-information .information .Commodity-name .commodity .name {
            margin-left: 20rpx;
}
.Information-container .Order-information .information .Commodity-name .commodity .name text:nth-child(1) {
              color: #000000;
}
.Information-container .Order-information .information .Commodity-name .commodity .name text:nth-child(2) {
              color: #888888;
}
.Information-container .Order-information .Li {
      margin: 30rpx 0 10rpx 0;
      height: 40rpx;
}
.Information-container .Order-information .Li .lis text:nth-child(1) {
        width: 120rpx;
        height: 40rpx;
        line-height: 40rpx;
        text-align: center;
}
.Information-container .Order-information .Li .lis text:nth-child(2) {
        margin-left: 22rpx;
        color: #999999;
}
.Information-container .Order-information .Li .coupon text:nth-child(1) {
        background: #FFCFCF;
        border: 1rpx solid #FF7F7F;
        color: #FF0000;
}
.Information-container .Order-information .Li .integral text:nth-child(1) {
        background: #B5EBFF;
        border: 1rpx solid #21B1CB;
        color: #1889E5;
}
.Information-container .Order-information .Li .commission text:nth-child(1) {
        background: #E2FFF5;
        border: 1rpx solid #188144;
        color: #118827;
}
.Information-container .Order-information .Li .commission text:nth-child(3) {
        margin-left: 34rpx;
        height: 32rpx;
        line-height: 17rpx;
        width: 32rpx;
        background: #69B7FF;
        border-radius: 50%;
}
.Information-container .Order-information .Li .text {
        margin-right: 24rpx;
        color: #FF0000;
}
.Information-container .Order-information .Commission {
      margin-top: 30rpx;
      padding: 22rpx 0 20rpx 0;
      border-top: 1rpx solid #E6E6E6;
      border-bottom: 1rpx solid #E6E6E6;
}
.Information-container .Order-information .Price {
      margin-top: 30rpx;
}
.Information-container .Order-information .Price .price {
        width: 530rpx;
        height: 30rpx;
        color: #888888;
}
.Information-container .Order-information .Price .price .text1 text:nth-child(2) {
          margin-left: 15rpx;
}
.Information-container .Order-information .Price .text {
        margin-right: 24rpx;
}
.Information-container .Delivery {
    margin: 34rpx 0 18rpx 0;
    padding: 10rpx 14rpx 24rpx 26rpx;
    width: 670rpx;
    background: #fff;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
            flex-direction: column;
}
.Information-container .Delivery > text {
    font-weight: bold;
    color: black;
    border-bottom: 1rpx solid #E6E6E6;
    line-height: 70rpx;
}
.Information-container .Delivery .ReceivingGoods, .Information-container .Delivery .address, .Information-container .Delivery .time {
    margin-top: 35rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
}
.Information-container .Delivery .ReceivingGoods .img {
    width: 35rpx;
    height: 37rpx;
}
.Information-container .Delivery .address .img {
    width: 32rpx;
    height: 42rpx;
}
.Information-container .Delivery .time .img {
    width: 38rpx;
    height: 38rpx;
}
.Information-container .Delivery .ReceivingGoods > text:nth-child(2), .Information-container .Delivery .address > text:nth-child(2), .Information-container .Delivery .time > text:nth-child(2) {
    margin-left: 25rpx;
    width: 88rpx;
    font-weight: 300;
    color: black;
}
.Information-container .Delivery .address > text:nth-child(3) {
    width: 410rpx;
    font-weight: 300;
    color: black;
}
.Information-container .Delivery .ReceivingGoods .contact, .Information-container .Delivery .address > text:nth-child(3), .Information-container .Delivery .time > text:nth-child(3) {
    margin-left: 54rpx;
}
.Information-container .Delivery .ReceivingGoods .contact > text {
    font-weight: 300;
    color: black;
}
.Information-container .Delivery .ReceivingGoods .contact > text:nth-child(2) {
    margin-left: 40rpx;
}
.Information-container .Delivery .time > text:nth-child(3) {
    font-weight: 300;
    color: #999999;
}
.Information-container .Delivery-time .li {
    margin: 30rpx 0 15rpx 0;
}
.Information-container .Delivery-time .li text:nth-child(1) {
      width: 112rpx;
}
.Information-container .Delivery-time .li text:nth-child(2) {
      margin-left: 75rpx;
}
.Information-container .Delivery-time .li text:nth-child(3) {
      margin-left: 10rpx;
}
.Information-container .Delivery-time .evaluate {
    margin-top: 15rpx;
    height: 78rpx;
}
.Information-container .Delivery-time .evaluate .text1 text {
      width: 88rpx;
      height: 55rpx;
      line-height: 55rpx;
      text-align: center;
      margin: 0 9rpx;
}
.Information-container .Delivery-time .evaluate .text2 {
      width: 248rpx;
      margin-right: 20rpx;
}
.Information-container .evaluate {
    margin: 20rpx 0;
}
.Information-container .evaluate .text {
      height: 114rpx;
}
.Information-container .evaluate .text text:nth-child(1) {
        margin-left: 26rpx;
}
.Information-container .evaluate .text text:nth-child(2) {
        margin-right: 40rpx;
}
.Information-container .Complaint .text {
    height: 114rpx;
}

